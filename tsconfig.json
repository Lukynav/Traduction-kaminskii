{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@meta/*": ["src/meta/*"],
      "@pages/*": ["src/pages/*"],
      "@models/*": ["src/models/*"],
      "@icons/*": ["src/icons/*"],
      "@views/*": ["src/views/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
